{
    "type": "sqlite",
    "database": "build/syncify.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": ["build/data/entity/**/*.ts"],
    "migrations": ["build/data/migration/**/*.ts"],
    "subscribers": ["build/data/subscriber/**/*.ts"],
    "cli": {
        "entitiesDir": "build/data/entity",
        "migrationsDir": "build/data/migration",
        "subscribersDir": "build/data/subscriber"
    }
}

