{
    "name": "syncify",
    "version": "1.0.0",
    "description": "Syncify",
    "main": "src/app.js",
    "scripts": {
        "build": "npm run clean && env NODE_ENV=production tsc",
        "build:dev": "npm run clean && env NODE_ENV=development tsc -w",
        "clean": "rm -rf ./build",
        "format": "prettier --write \"./src/**/*.{js,ts,json}\"",
        "start": "npm run build && env NODE_ENV=production  node build/app.js",
        "start:dev": "env NODE_ENV=development nodemon build/app.js"
    },
    "author": "",
    "contributors": [
        "Arnav Gosain <>",
        "Ishan Saxena <>"
    ],
    "license": "MIT",
    "dependencies": {
        "@feathersjs/express": "^4.5.8",
        "@feathersjs/feathers": "^4.5.8",
        "axios": "^0.19.2",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "discord.js": "^12.3.1",
        "dotenv": "^8.2.0",
        "firebase": "^7.18.0",
        "reflect-metadata": "^0.1.10",
        "request": "^2.88.2",
        "sqlite3": "^4.0.3",
        "telegram-bot-api": "^2.0.1",
        "typeorm": "0.2.25"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.7",
        "@types/node": "14.6.0",
        "@types/node-telegram-bot-api": "^0.50.2",
        "@types/request": "^2.48.5",
        "@types/sqlite3": "^3.1.6",
        "@types/ws": "^7.2.6",
        "babel-loader": "^8.1.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "ts-loader": "^8.0.2",
        "typescript": "3.9.7",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.5.1"
    },
    "prettier": {
        "printWidth": 80,
        "trailingComma": "es5",
