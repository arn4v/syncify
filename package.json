{
  "name": "syncify",
  "version": "1.0.0",
  "description": "Syncify",
  "main": "src/app.js",
  "scripts": {
    "build": "npm run clean && env NODE_ENV=production tsc",
    "build:dev": "npm run clean && env NODE_ENV=development tsc -w --sourceMap true",
    "clean": "rm -rf ./build",
    "format": "prettier --write \"./src/**/*.{js,ts,json}\"",
    "start": "ts-node src/index.ts",
    "start:dev": "env NODE_ENV=development nodemon build/app.js"
  },
  "author": "",
  "contributors": [
    "Arnav Gosain",
    "Ishan Saxena"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sqlite3": "^5.0.0",
    "telegram-bot-api": "^2.0.1",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/cron": "^1.7.2",
    "@types/node": "^16.4.3",
    "@types/node-telegram-bot-api": "^0.51.1",
    "@types/request": "^2.48.5",
    "@types/sqlite3": "^3.1.6",
    "@types/ws": "^7.4.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuriousCatProjects/syncify-server.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/CuriousCatProjects/syncify-server/issues"
  },
  "homepage": "https://github.com/CuriousCatProjects/syncify-server#readme"
