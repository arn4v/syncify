{
    "type": "sqlite",
    "database": "./build/syncify.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": ["src/data/typeorm/entity/**/*.ts"],
    "migrations": ["src/data/typeorm/migration/**/*.ts"],
    "subscribers": ["src/data/typeorm/subscriber/**/*.ts"],
    "cli": {
        "entitiesDir": "src/data/typeorm/entity",
        "migrationsDir": "src/data/typeorm/migration",
        "subscribersDir": "src/data/typeorm/subscriber"
    }
}

